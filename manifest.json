{
  "manifest_version": 3,
  "name": "YouTube Video Speed Controller",
  "version": "1.0",
  "description": "Control the playback speed of HTML5 videos on YouTube.",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
